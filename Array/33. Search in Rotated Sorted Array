class Solution:
    def search(self, nums: List[int], target: int) -> int:
        if not nums:
             return -1
        l = len(nums)
        start, end = 0, l-1
        while end >= start:
            mid = (end + start)//2
            if nums[mid] == target:
                return mid
            if nums[mid] >= nums[start]:
                if target >= nums[start] and target < nums[mid]:
                    end = mid - 1
                else:
                    start = mid + 1
            else:
                if target > nums[mid] and target <= nums[end]:
                    start = mid + 1
                else:
                    end = mid - 1
        return -1
