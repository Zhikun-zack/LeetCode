class Solution {
    public int numTrees(int n) {
        if( n < 1 )
            return 0;
        return createBST(1, n).size();
    }
    public List<TreeNode> createBST(int start, int end){
        List<TreeNode> res = new ArrayList<>();
        if (end < start){
            res.add(null);
            return res;
        }
            
        for (int i = start; i <= end; i++){
            List<TreeNode> lefts = createBST(start, i-1);
            List<TreeNode> rights = createBST(i+1, end);

            for (TreeNode left: lefts){
                for(TreeNode right: rights){
                    TreeNode root = new TreeNode(i);
                    root.left = left;
                    root.right = right;
                    res.add(root);
                }
            }
        }
        return res;
    }
}
